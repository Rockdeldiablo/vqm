Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"ListArgColorPlot6", "[", "list_", "]"}], ":=", 
    RowBox[{"With", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"intfun", "=", 
        RowBox[{"Interpolation", "@", "list"}]}], "}"}], ",", 
      RowBox[{"Style", "[", 
       RowBox[{
        RowBox[{"ListLinePlot", "[", 
         RowBox[{
          RowBox[{"Abs", "@", "list"}], ",", 
          RowBox[{"Filling", "\[Rule]", "Bottom"}], ",", 
          RowBox[{"ColorFunctionScaling", "\[Rule]", "False"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"Epilog", "\[Rule]", 
           RowBox[{"(", 
            RowBox[{"FF", "=", 
             RowBox[{"First", "[", 
              RowBox[{"ListLinePlot", "[", 
               RowBox[{"Abs", "@", "yvalues"}], "]"}], "]"}]}], ")"}]}], ",", 
          RowBox[{"Axes", "\[Rule]", "False"}], ",", "\[IndentingNewLine]", 
          RowBox[{"ColorFunction", "\[RuleDelayed]", 
           RowBox[{"Function", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"x", ",", "y"}], "}"}], ",", 
             RowBox[{"(", 
              RowBox[{"Hue", "@", 
               RowBox[{"(", 
                RowBox[{"Mod", "[", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"Arg", "[", 
                    RowBox[{"intfun", "[", "x", "]"}], "]"}], "/", 
                   RowBox[{"(", 
                    RowBox[{"2", "Pi"}], ")"}]}], ",", "1"}], "]"}], ")"}]}], 
